{
  "name": "cmms-enterprise-backend",
  "version": "1.3.0-beta.2",
  "description": "Enterprise CMMS SaaS Platform - Backend",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "db:generate": "drizzle-kit generate:pg --config=drizzle.config.mjs",
    "db:push": "drizzle-kit push:pg --config=drizzle.config.mjs",
    "db:migrate": "drizzle-kit push:pg --config=drizzle.config.mjs",
    "db:studio": "drizzle-kit studio --config=drizzle.config.mjs",
    "db:seed": "tsx src/db/seed.ts",
    "lint": "eslint src",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "cmms",
    "maintenance",
    "saas",
    "multi-tenant"
  ],
  "author": "CMMS Enterprise",
  "license": "MIT",
  "dependencies": {
    "@elastic/elasticsearch": "^9.2.1",
    "@sendgrid/mail": "^8.1.6",
    "bcrypt": "^5.1.0",
    "bull": "^4.16.5",
    "bull-board": "^1.7.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "drizzle-kit": "^0.19.13",
    "drizzle-orm": "^0.28.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
      "multer": "^2.0.2",
    "ioredis": "^5.9.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node-cron": "^4.2.1",
    "pdf-lib": "^1.17.1",
    "pg": "^8.10.0",
    "redis": "^5.10.0",
    "socket.io": "^4.8.3",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.0",
    "winston": "^3.10.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.20",
    "@types/multer": "^1.4.12",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.8.0",
    "@types/pg": "^8.10.7",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^8.50.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tsx": "^3.13.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  }
